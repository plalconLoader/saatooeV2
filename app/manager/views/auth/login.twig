<!DOCTYPE html>
<html lang="en">
<head>
    <meta http-equiv="content-type" content="text/html; charset=UTF-8">
    <meta charset="utf-8">
    <title>
        善土e配登录中心
    </title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1, user-scalable=no">
    <meta name="description" content="">
    <meta name="author" content="">
    <!-- STYLESHEETS -->
    <!--[if lt IE 9]>
    <script src="js/flot/excanvas.min.js"></script>
    <script src="http://html5shiv.googlecode.com/svn/trunk/html5.js"></script>
    <script src="http://css3-mediaqueries-js.googlecode.com/svn/trunk/css3-mediaqueries.js"></script>
    <![endif]-->

    <link rel="stylesheet" type="text/css" href="/dist/login/cloud-admin.css" >

    <!-- DATE RANGE PICKER -->
    <link rel="stylesheet" type="text/css" href="/dist/login/daterangepicker-bs3.css" />
    <!-- UNIFORM -->
    <link rel="stylesheet" type="text/css" href="/dist/login/uniform.default.min.css" />
    <!-- ANIMATE -->
    <link rel="stylesheet" type="text/css" href="/dist/login/animate.min.css" />
    <link rel="stylesheet" href="/dist/css/font-awesome.min.css">
    <!-- FONTS -->
    {#<link href='http://fonts.useso.com/css?family=Open+Sans:300,400,600,700' rel='stylesheet' type='text/css'>#}
</head>
<body class="login">
<!-- PAGE -->
<section id="page">
    <!-- HEADER -->
    <header>
        <!-- NAV-BAR -->
        <div class="container">
            <div class="row">
                <div class="col-md-4 col-md-offset-4">
                    <div id="logo">
                        {#<a href="index.html">#}
                        {#logo区域#}
                            <div style="margin-bottom: 130px;"></div>
                        {#</a>#}
                    </div>
                </div>
            </div>
        </div>
        <!--/NAV-BAR -->
    </header>
    <!--/HEADER -->
    <!-- LOGIN -->
    <section id="login" class="visible">
        <div class="container">
            <div class="row">
                <div class="col-md-4 col-md-offset-4">
                    <div class="login-box-plain">
                        <h2 class="bigintro">
                            善土e配
                        </h2>
                        <div class="divide-40"></div>
                        <form role="form" method="post">
                            <div class="form-group">
                                <label for="exampleInputEmail1">
                                    帐号
                                </label>
                                <i class="fa fa-user"></i>
                                <input type="text" required name="username" class="form-control" id="exampleInputEmail1" >
                            </div>
                            <div class="form-group">
                                <label for="exampleInputPassword1">
                                    密码
                                </label>
                                <i class="fa fa-lock"></i>
                                <input type="password" required name="password" class="form-control" id="exampleInputPassword1" >
                            </div>

                            <div class="form-actions">

                                <button type="submit" class="btn btn-success btn-block">
                                    登录
                                </button>
                            </div>
                        </form>
                        <!-- SOCIAL LOGIN -->
                        <div class="divide-20"></div>
                        {#<div class="center">#}
                            {#<strong>Or login using your social account</strong>#}
                        {#</div>#}
                        <div class="divide-20"></div>
                        {#<div class="social-login center">#}
                            {#<a class="btn btn-primary btn-lg">#}
                                {#<i class="fa fa-facebook"></i>#}
                            {#</a>#}
                            {#<a class="btn btn-info btn-lg">#}
                                {#<i class="fa fa-twitter"></i>#}
                            {#</a>#}
                            {#<a class="btn btn-danger btn-lg">#}
                                {#<i class="fa fa-google-plus"></i>#}
                            {#</a>#}
                        {#</div>#}
                        <!-- /SOCIAL LOGIN -->

                    </div>
                </div>
            </div>
        </div>
    </section>
    <!--/LOGIN -->
</section>
<!--/PAGE -->
<!-- JAVASCRIPTS -->
<!-- Placed at the end of the document so the pages load faster -->
<!-- JQUERY -->
<script src="/plugins/jQuery/jquery-2.2.3.min.js"></script>
<!-- JQUERY UI-->
<script src="/dist/login/jquery-ui-1.10.3.custom.min.js"></script>
<!-- BOOTSTRAP -->
<script src="/dist/login/bootstrap.min.js"></script>


<!-- UNIFORM -->
<script type="text/javascript" src="/dist/login/jquery.uniform.min.js"></script>
<!-- CUSTOM SCRIPT -->
<script src="/dist/login/script.js"></script>
<script>
    jQuery(document).ready(function() {
        App.setPage("login");  //Set current page
        App.init(); //Initialise plugins and elements
    });
</script>
<script type="text/javascript">
    function swapScreen(id) {
        jQuery('.visible').removeClass('visible animated fadeInUp');
        jQuery('#'+id).addClass('visible animated fadeInUp');
    }
</script>
<!-- /JAVASCRIPTS -->
</body>
</html>